{
  "name": "@kaola/hermes",
  "version": "0.3.1",
  "description": "Apollo SDK for javascript",
  "files": [
    "src",
    "dist/*.js"
  ],
  "main": "dist/hermes.js",
  "module": "dist/hermes.esm.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged",
    "dev": "rollup -w -c scripts/config.js --environment TARGET:umd",
    "dev:esm": "rollup -w -c scripts/config.js --environment TARGET:esm",
    "dev:cjs": "rollup -w -c scripts/config.js --environment TARGET:cjs",
    "build": "node scripts/build.js",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/everywill/hermes.git"
  },
  "author": "everywill",
  "license": "ISC",
  "dependencies": {
    "crypto-js": "^3.1.9-1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-config-kaola": "^0.1.3",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "np": "^3.0.4",
    "prettier": "^1.12.1",
    "prettier-eslint-cli": "^4.7.1",
    "rollup": "^0.63.4",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "uglify-js": "^3.4.6",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "zlib": "^1.0.5"
  },
  "publishConfig": {
    "registry": "http://rnpm.hz.netease.com/"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
